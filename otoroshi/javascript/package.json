    "fuzzy": "0.1.3",
    "get-value": "3.0.1",
    "highlight.js": "11.10.0",
    "jquery": "3.7.1",
    "legally": "3.5.10",
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "monaco-editor": "^0.53.0",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "query-string": "9.1.1",
    "react": "17.0.2",
    "react-ace": "12.0.0",
